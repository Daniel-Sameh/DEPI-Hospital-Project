{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "./public/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "./uploads/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "./node_modules/admin-lte/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "./index.js"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "/uploads/$1"
      },
      {
        "src": "/(.*)",
        "dest": "./index.js"
      }
    ]
  }