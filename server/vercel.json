{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/adminlte/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/fontawesome/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "PORT": "5000"
  }
}
