{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/adminlte/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/fontawesome/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev"
}
